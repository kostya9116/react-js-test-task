{
  "name": "react-js-test-task",
  "version": "1.0.0",
  "description": "Web application that gives the user the ability to see the available products, add new ones and edit the existing products.",
  "private": true,
  "keywords": [],
  "author": "Konstantin Harutyunyan",
  "license": "MIT",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start config/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls",
    "build": "`npm bin`/webpack",
    "dev": "webpack-dev-server --inline --hot",
    "generate": "generate",
    "generate:component": "npm run generate component",
    "generate:container": "npm run generate container",
    "generate:page": "npm run generate page",
    "lint": "`npm bin`/eslint . --ext .js --ext .jsx; exit 0",
    "deploy": "NODE_ENV=production `npm bin`/webpack -p",
    "start": "npm run dev",
    "clean": "rm -rf app/dist app/build",
    "setup": "npm install"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.1",
    "react-redux": "5.0.3",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.7",
    "redux": "3.6.0",
    "redux-action": "^1.2.2",
    "redux-logger": "3.0.0",
    "redux-promise-middleware": "4.2.0",
    "redux-thunk": "2.2.0",
    "semantic-ui-react": "^0.67.2",
    "store": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-scripts": "0.8.5",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.16.3"
  }
}
